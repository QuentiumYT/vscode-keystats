{
  "name": "vscode-keystats",
  "displayName": "KeyStats",
  "description": "Key press count, combo and metrics about the keys you press the most.",
  "version": "0.2.0",
  "publisher": "QuentiumYT",
  "author": {
    "name": "Quentin Lienhardt"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "keypress",
    "statistics",
    "key count",
    "combo",
    "keys",
    "productivity",
    "tracking",
    "tracker"
  ],
  "homepage": "https://github.com/QuentiumYT/vscode-keystats",
  "bugs": {
    "url": "https://github.com/QuentiumYT/vscode-keystats/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QuentiumYT/vscode-keystats.git"
  },
  "license": "MIT",
  "icon": "images/logo.png",
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  }
}
